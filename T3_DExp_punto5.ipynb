  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1] 26.00000 34.66667 39.66667 49.33333 42.33333 37.66667 54.66667 42.33333\n"
     ]
